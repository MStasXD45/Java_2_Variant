package Ad.Muzyka;
class Cart {
    private int mSize;
    private Item[] items = new Item[];
    private int top;

    public Cart(int m) {
        this.mSize = m;
        items[] =new Item()[mSize];
        top = -1;
    }

    public void addElement(int element) {
        items[++top] = element;
    }
}
